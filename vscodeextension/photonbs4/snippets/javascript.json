{
    "Photon Javascript Loop": {
        "prefix": "!jsloop",
        "body": [
            "for (let ${index}=0; ${index} < ${array}.length; ${index}++) {",
            "\t${element} = ${array}[${index}]",
            "\t$0",
            "}"
        ]
    },
    "Photon Javascript Console Log": {
        "prefix": "!console",
        "body": [
            "console.${1|log,warning,error|}('$2');",
            "$0"
        ]
    }
}