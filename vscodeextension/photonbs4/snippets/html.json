{
    "Photon Button Group": {
        "prefix": "!buttongroup",
        "body": [
            "<div class=\"btn-group\">",
                "\t<button type=\"button\" class=\"btn btn-primary\">$1</button>",
                "\t<button type=\"button\" class=\"btn btn-primary\">$2</button>",
                "\t<button type=\"button\" class=\"btn btn-primary\">$3</button>",
            "</div>",
            "$0"
        ]
    },
    "Photon Button Badge": {
        "prefix": "!buttonbadge",
        "body": [
            "<button type=\"button\" class=\"btn btn-primary\">",
            "\t<span>$1</span>",
            "\t<span class=\"badge badge-light\">$2</span>",
            "</button>",
            "$0"
        ]
    },
    "Photon Pagination": {
        "prefix": "!pagination",
        "body": [
            "<ul class=\"pagination\">",
            "\t<li class=\"page-item\"><a class=\"page-link\" href=\"#\">Previous</a></li>",
            "\t<li class=\"page-item\"><a class=\"page-link\" href=\"#\">$1</a></li>",
            "\t<li class=\"page-item\"><a class=\"page-link\" href=\"#\">$2</a></li>",
            "\t<li class=\"page-item\"><a class=\"page-link\" href=\"#\">$3</a></li>",
            "\t<li class=\"page-item\"><a class=\"page-link\" href=\"#\">Next</a></li>",
            "</ul>",
            "$0"
        ]
    },
    "Photon Card": {
        "prefix": "!card",
        "body": [
            "<div class=\"card\">",
            "\t<div class=\"card-header\">${header}</div>",
            "\t<div class=\"card-body\">${content}</div> ",
            "\t<div class=\"card-footer\">${footer}</div>",
            "</div>",
            "$0"
        ]
    },
    "Photon Card Link": {
        "prefix": "!cardlink",
        "body": [
            "<div class=\"card\">",
            "\t<div class=\"card-body\">",
            "\t\t<h4 class=\"card-title\">${title}</h4>",
            "\t\t<p class=\"card-text\">${text}</p>",
            "\t\t<a href=\"${linkOne}\" class=\"card-link\">${linkDescriptionOne}</a>",
            "\t\t<a href=\"${linkTwo}\" class=\"card-link\">${linkDescriptionTwo}</a>",
            "\t</div>",
            "</div>",
            "$0"
        ]
    }, 
    "Photon Card Image": {
        "prefix": "!cardimage",
        "body": [
            "<div class=\"card\" style=\"width:400px\">",
            "\t<img class=\"card-img-top\" src=\"${cardImage}\" alt=\"${altCardImage}\">",
            "\t<div class=\"card-body\">",
            "\t\t<h4 class=\"card-title\">${title}</h4>",
            "\t\t<p class=\"card-text\">${text}</p>",
            "\t\t<a href=\"#\" class=\"btn btn-${color}\">${button}</a>",
            "\t</div>",
            "</div>",
            "$0"
        ]
    },
    "Photon Card Image Overlay": {
        "prefix": "!cardimageoverlay",
        "body": [
            "<div class=\"card\" style=\"width:500px\">",
            "\t<img class=\"card-img-top\" src=\"${image}\" alt=\"${imagealt}\">",
            "\t<div class=\"card-img-overlay\">",
            "\t\t<h4 class=\"card-title\">${title}</h4>",
            "\t\t<p class=\"card-text\">${text}</p>",
            "\t\t<a href=\"${link}\" class=\"btn btn-${color}\">${linktext}</a>",
            "\t</div>",
            "</div>",
            "$0"
        ]
}